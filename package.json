{
  "bin": {
    "swaxios": "dist/cli.js"
  },
  "dependencies": {
    "axios": "0.19.0",
    "ci-info": "2.0.0",
    "cli-interact": "0.1.9",
    "commander": "2.20.0",
    "fs-extra": "8.1.0",
    "handlebars": "4.1.2",
    "handlebars-helpers": "0.10.0",
    "prettier": "1.18.2",
    "swagger-parser": "6.0.5",
    "yamljs": "0.3.0"
  },
  "description": "Generate Swagger API clients with axios",
  "devDependencies": {
    "@types/ci-info": "2.0.0",
    "@types/cli-interact": "0.1.0",
    "@types/fs-extra": "8.0.0",
    "@types/handlebars-helpers": "0.5.2",
    "@types/jasmine": "3.3.15",
    "@types/minimist": "1.2.0",
    "@types/node": "12.6.6",
    "@types/prettier": "1.18.0",
    "@types/swagger-parser": "4.0.3",
    "@types/yamljs": "0.2.30",
    "@wireapp/prettier-config": "0.3.0",
    "@wireapp/tslint-config": "1.4.1",
    "cpx": "1.5.0",
    "generate-changelog": "1.8.0",
    "husky": "3.0.1",
    "jasmine": "3.4.0",
    "lint-staged": "9.2.0",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.5.3"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "*.{json,md,scss,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "swaxios",
  "prettier": "@wireapp/prettier-config",
  "scripts": {
    "changelog:commit": "git add CHANGELOG.md && git commit -m \"docs: updated CHANGELOG.md\"",
    "clean": "rimraf dist",
    "copy": "cpx src/templates/**/* dist/templates/",
    "dist": "yarn clean && tsc && yarn copy",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:other && yarn lint:ts",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts?(x)\"",
    "postversion": "git push origin && git push origin --tags && npm publish",
    "prettier": "prettier \"**/*.{json,md}\"",
    "preversion": "git pull && yarn && yarn test && yarn dist",
    "release:patch": "changelog -p -x \"chore,docs,refactor,style,test\" && yarn changelog:commit && npm version patch",
    "start": "ts-node src/cli.ts -i ./src/test/fixtures/wire-sso.json -o ./src/temp -f",
    "test": "yarn lint && yarn test:node",
    "test:node": "nyc jasmine --config=jasmine.json"
  },
  "version": "0.0.15"
}
